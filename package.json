{
  "name": "bot",
  "scripts": {
    "start": "node index.js",
    "docker": "docker-compose up",
    "watch": "start npm run nodemon && start npm run tailwind && start npm run scss",
    "nodemon": "nodemon -e .js,.twig index.js",
    "scss": "sass --watch website/scss/app.scss:website/public/styles/app.css website/scss/tailwind.scss:website/public/styles/tailwind.css --style compressed --no-source-map",
